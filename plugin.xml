<?xml version='1.0' encoding='utf-8'?>
<plugin id="com-amadeus-helloworld" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>OwnHelloWorld</name>
    <js-module name="OwnHelloWorld" src="www/OwnHelloWorld.js">
        <clobbers target="OwnHelloWorld" />
    </js-module>
    
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="OwnHelloWorld">
                <param name="ios-package" value="OwnHelloWorld" />
            </feature>
        </config-file>
        <source-file src="src/ios/OwnHelloWorld.swift" />
    </platform>
    
</plugin>
